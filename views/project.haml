%h1= @proj.name
%h2 Log Messages
.logContainer
	%ul
		- @logs.each do |l|
			%li
				.logEntry
					%span.timestamp= l.timestamp.httpdate
					%span.logmessage= l.entry
					
%h2 Power Supplies
.supplyContainer
	%ul
		- @supplies.each do |s|
			%li
				.supplyEntry
					%span.timestamp= s.timestamp.httpdate
					%span.logmessage= s.name
					%span.logmessage= s.ip
					%span.logmessage= s.sn
					